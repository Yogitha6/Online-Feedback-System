<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>OnlineFeedbackSystem</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<display-name>AddTrainingSchedule</display-name>
		<servlet-name>AddTrainingSchedule</servlet-name>
		<servlet-class>com.ca.ofs.servlets.AddTrainingSchedule</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddTrainingSchedule</servlet-name>
		<url-pattern>/AddTrainingSchedule</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginHandler</servlet-name>
		<servlet-class>com.ca.ofs.servlets.LoginHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginHandler</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LogoutHandler</servlet-name>
		<servlet-class>com.ca.ofs.servlets.LogoutHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LogoutHandler</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DownloadHandler</servlet-name>
		<servlet-class>com.ca.ofs.servlets.DownloadHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DownloadHandler</servlet-name>
		<url-pattern>/downloadReport</url-pattern>
	</servlet-mapping>
	
	

	<servlet>
		<servlet-name>GetFeedbackHandler</servlet-name>
		<servlet-class>com.ca.ofs.servlets.GetFeedbackHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetFeedbackHandler</servlet-name>
		<url-pattern>/getFeedbackForm</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SubmitFeedbackHandler</servlet-name>
		<servlet-class>com.ca.ofs.servlets.SubmitFeedbackHandler</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SubmitFeedbackHandler</servlet-name>
		<url-pattern>/submitfeedback</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddCoordinator</servlet-name>
		<servlet-class>com.ca.ofs.servlets.AddCoordinatorServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddCoordinator</servlet-name>
		<url-pattern>/addcoordinator</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddSchedule</servlet-name>
		<servlet-class>com.ca.ofs.servlets.AddTrainingSchedule</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddSchedule</servlet-name>
		<url-pattern>/addschedule</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReportPerTraining</servlet-name>
		<servlet-class>com.ca.ofs.servlets.ReportPerTraining</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReportPerTraining</servlet-name>
		<url-pattern>/reportPerTraining</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReportPerTrainer</servlet-name>
		<servlet-class>com.ca.ofs.servlets.ReportPerTrainer</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReportPerTrainer</servlet-name>
		<url-pattern>/reportPerTrainer</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReportAllTrainings</servlet-name>
		<servlet-class>com.ca.ofs.servlets.ReportAllTrainings</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReportAllTrainings</servlet-name>
		<url-pattern>/reportAllTrainings</url-pattern>
	</servlet-mapping>

</web-app>